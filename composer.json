{
	"repositories": {
		"packagist": {
			"type": "composer",
			"url": "https://packagist.phpcomposer.com"
		}
	},
    "name": "xue-y/cas-php",
    "description": "Convenient and succinct PHP framework",
    "version": "1.0.6-dev",
    "type": "project",
    "license": "Apache-2.0",
    "minimum-stability":"dev",
    "authors": [
        {
            "name": "xue-y",
            "email": "php.develop@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "topthink/framework": "5.1.*",
		"topthink/think-helper":"*",
        "topthink/think-captcha":"*"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "extra": {
        "think-path": "thinkphp"
    },
    "config": {
        "preferred-install": "dist"
    }
}
